{
  "Version": "2022-11-11",
  "Id": "allow-sns-to-sqs",
  "Statement": [
    {
      "Sid": "bemobi-sqs-sid",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "SQS:SendMessage",
      "Resource": "arn:aws:sqs:us-east-1:000000000000:bemobi-sqs",
      "Condition": {
        "ArnEquals": {
          "aws:SourceArn": "arn:aws:sns:us-east-1:000000000000:bemobi-sns"
        }
      }
    }
  ]
}