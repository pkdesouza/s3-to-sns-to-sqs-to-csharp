{
  "Version": "2022-11-11",
  "Id": "allow-s3-to-sns",
  "Statement": [
    {
      "Sid": "bemobi-sid",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "SNS:Publish",
      "Resource": "arn:aws:sns:us-east-1:000000000000:bemobi-sns",
      "Condition": {
        "ArnEquals": {
          "aws:SourceArn": "arn:aws:s3:::bemobi"
        }
      }
    }
  ]
}